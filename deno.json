{
  "workspace": [
    "./session"
  ],
  "tasks": {
    "check": "deno fmt --check && deno check **/src && deno lint **/src && deno test -A",
    "compliance": "deno run -A tools/compliance.ts"
  },
  "unstable": [
    "kv"
  ],
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@std/path": "jsr:@std/path@^1.1.3",
    "preact": "npm:preact@^10.27.2",
    "@felix/argon2": "jsr:@felix/argon2@^3.0.2",
    "@innovatedev/fresh-session": "./session/src/mod.ts",
    "@innovatedev/fresh-session/kv-store": "./session/src/stores/kv.ts",
    "@innovatedev/fresh-session/memory-store": "./session/src/stores/memory.ts",
    "@olli/kvdex": "jsr:@olli/kvdex@^3.4.2"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  },
  "license": "MIT",
  "tags": [
    "ai-generated",
    "ai-assisted"
  ]
}
